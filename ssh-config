# Needed at Curity HQ
Host *
IPQoS=throughput

#Using multiple Bitbucket accounts
#urre Bitbucket
Host bitbucket.org-urre
  HostName bitbucket.org
  User git
  IdentityFile ~/.ssh/urre-bitbucket
  IdentitiesOnly yes

#urbansanden Bitbucket
Host bitbucket.org-urbansanden
  HostName bitbucket.org
  User git
  IdentityFile ~/.ssh/urbansanden-bitbucket
  IdentitiesOnly yes
