{
  "explorer.confirmDelete" : false,
  "search.useIgnoreFiles" : true,
  "editor.fontWeight" : "300",
  "workbench.enableExperiments" : false,
  "emmet.includeLanguages" : {
    "javascript" : "javascriptreact"
  },
  "editor.accessibilitySupport" : "off",
  "diffEditor.ignoreTrimWhitespace" : true,
  "[javascript]" : {
    "editor.defaultFormatter" : "esbenp.prettier-vscode"
  },
  "editor.guides.bracketPairs" : true,
  "[javascriptreact]" : {
    "editor.defaultFormatter" : "esbenp.prettier-vscode"
  },
  "editor.fontFamily" : "'JetBrainsMono-Regular', 'IBM Plex Mono', 'Noto Mono', 'SF Mono', Menlo, Monaco, 'Courier New', monospace",
  "workbench.startupEditor" : "newUntitledFile",
  "[css]" : {
    "editor.defaultFormatter" : "esbenp.prettier-vscode"
  },
  "[typescript]" : {
    "editor.defaultFormatter" : "esbenp.prettier-vscode"
  },
  "terminal.integrated.env.osx" : {
    "FIG_NEW_SESSION": "1"
  },
  "editor.formatOnPaste" : false,
  "terminal.integrated.fontFamily" : "'JetBrainsMono-Regular', 'IBM Plex Mono', 'Noto Mono', 'SF Mono', Menlo, Monaco, 'Courier New', monospace",
  "search.exclude" : {
    "tmp\/**" : true,
    "assets\/build\/" : true,
    "assets\/rev\/" : true,
    ".asset-cache" : true,
    "**\/.git" : true,
    "node_modules" : true,
    "_site" : true
  },
  "editor.formatOnSave" : false,
  "editor.renderWhitespace" : "all",
  "[jsonc]" : {
    "editor.defaultFormatter" : "esbenp.prettier-vscode"
  },
  "files.trimTrailingWhitespace" : true,
  "files.defaultLanguage" : "html",
  "keyboard.touchbar.enabled" : false,
  "workbench.statusBar.visible" : true,
  "editor.codeActionsOnSave" : {

  },
  "editor.cursorBlinking" : "phase",
  "editor.renderControlCharacters" : true,
  "editor.tabSize" : 2,
  "terminal.integrated.fontSize" : 16,
  "extensions.ignoreRecommendations" : true,
  "javascript.updateImportsOnFileMove.enabled" : "always",
  "[json]" : {
    "editor.defaultFormatter" : "vscode.json-language-features"
  },
  "window.openFoldersInNewWindow" : "on",
  "workbench.tips.enabled" : false,
  "window.title": "${activeEditorMedium}",
  "files.insertFinalNewline" : true,
  "editor.trimAutoWhitespace" : true,
  "editor.bracketPairColorization.enabled" : true,
  "editor.insertSpaces" : false,
  "files.watcherExclude" : {
    "**\/node_modules\/**" : true,
    "**\/.git\/objects\/**" : true
  },
  "explorer.compactFolders" : false,
  "htmlEndTagLabels.labelColor" : "#495057",
  "editor.fontSize" : 16,
  "window.restoreWindows" : "all",
  "[scss]" : {
    "editor.defaultFormatter" : "esbenp.prettier-vscode"
  },
  "explorer.openEditors.visible" : 0,
  "editor.detectIndentation" : true,
  "files.trimFinalNewlines" : true,
  "explorer.confirmDragAndDrop" : false,
  "window.newWindowDimensions" : "inherit",
  "[mdx]" : {
    "editor.lineHeight" : 26,
    "editor.wordWrapColumn" : 72,
    "editor.formatOnPaste" : false,
    "editor.lineNumbers" : "on",
    "editor.defaultFormatter" : null,
    "editor.fontSize" : 16,
    "editor.quickSuggestions" : {
      "strings" : "off",
      "other" : "off",
      "comments" : "off"
    },
    "editor.formatOnSave" : false,
    "editor.fontLigatures" : true,
    "editor.wordWrap" : "wordWrapColumn"
  },
  "breadcrumbs.enabled": true,
  "security.workspace.trust.untrustedFiles": "open",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
